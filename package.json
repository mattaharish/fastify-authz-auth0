{
  "name": "fastify-authz-auth0",
  "version": "1.0.4",
  "description": "Auth0 library to get token data and extract roles and permissions",
  "main": "plugin.js",
  "repository": "https://github.com/mattaharish/fastify-authz-auth0.git",
  "jest": {
    "clearMocks": true,
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "testEnvironment": "node"
  },
  "scripts": {
    "lint": "eslint .",
    "test": "LOG_LEVEL=silent jest"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "eslint": "^6.3.0",
    "eslint-config-prettier": "^6.1.0",
    "eslint-plugin-jest": "^22.16.0",
    "eslint-plugin-prettier": "^3.1.0",
    "jest": "^24.7.1",
    "prettier-eslint": "^9.0.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "es6": true,
      "jest/globals": true,
      "node": true
    },
    "plugins": [
      "jest",
      "prettier"
    ],
    "extends": [
      "plugin:prettier/recommended",
      "eslint:recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 2018
    }
  },
  "eslintIgnore": [
    "node_modules/*",
    "coverage/*",
    ".nyc_output/*"
  ],
  "publishConfig": {
    "registry": "http://registry.npmjs.org/"
  },
  "dependencies": {
    "fastify": "^2.8.0",
    "fastify-plugin": "^1.6.0",
    "jsonwebtoken": "^8.5.1"
  }
}
